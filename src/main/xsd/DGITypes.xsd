	<xs:complexType name="RechazoCFE_DGIType">
		<xs:sequence minOccurs="1">
			<xs:element name="Motivo" minOccurs="1">
				<xs:annotation>
					<xs:documentation>
						E01 Tipo y Nº de CFE ya fue reportado como anulado
						E02 CFE no tiene al menos una línea de detalle
						E03 Tipo y Nº de CFE ya existe en los registros
						E04 Tipo y Nº de CFE no se corresponden con el CAE
                        E05 Firma electrónica no es válida
                        E07 Fecha y hora de firma no es válida
                        E08 No coincide RUC de CFE y Complemento Fiscal
                        E09 RUC emisor y/o tipo de CFE no se corresponden con el CAE
					</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
                     <xs:restriction base="xs:string">
                                <xs:maxLength value="3"/>
                                <xs:enumeration value="E01"/>
                                <xs:enumeration value="E02"/>
                                <xs:enumeration value="E03"/>
                                <xs:enumeration value="E04"/>
                                <xs:enumeration value="E05"/>
                                <xs:enumeration value="E07"/>
                                <xs:enumeration value="E08"/>
                                <xs:enumeration value="E09"/>
                      </xs:restriction>

                          </xs:simpleType>
                         </xs:element>
			<xs:element name="Glosa" minOccurs="1">
				<xs:simpleType>
					<xs:restriction base="ns1:Glosa50Type">

						<xs:enumeration value="Tipo y Nº de CFE ya fue reportado como anulado"/>
						<xs:enumeration value="Tipo y Nº de CFE ya existe en los registros"/>
						<xs:enumeration value="Tipo y Nº de CFE no se corresponden con el CAE"/>
						<xs:enumeration value="Firma electrónica no es válida"/>
						<xs:enumeration value="No cumple validaciones de Formato comprobantes"/>
						<xs:enumeration value="Fecha Firma de CFE no se corresponde con fecha CAE"/>
						<xs:enumeration value="No coincide RUC de CFE y Complemento Fiscal"/>
						<xs:enumeration value="RUC emisor y/o tipo de CFE no se corresponden con el CAE"/>
					</xs:restriction>
				</xs:simpleType>
